{
	"manifest_version": 2,
	"name": "DarkLeetCode",
	"description": "Turn Leetcode in Dark mode",
	"version": "1.0.0",
	"icons": {
		"16": "icon16.ico",
		"48": "icon48.ico",
		"128": "icon128.ico"
	},
	"content_scripts": [
		{
			"css": ["root.css"],
			"matches": ["https://leetcode.com/*"]
		},
		{
			"css": ["problemset.css"],
			"matches": ["https://leetcode.com/problemset/*"]
		},
		{
			"css": ["problem-discuss.css", "hljs-theme.css"],
			"matches": ["https://leetcode.com/discuss/*"]
		},
		{
			"css": ["problem.css", "problem-discuss.css", "hljs-theme.css"],
			"matches": ["https://leetcode.com/problems/*"]
		},
		{
			"css": ["notifications.css"],
			"matches": ["https://leetcode.com/notifications/*"]
		},
		{
			"css": ["explore.css"],
			"matches": ["https://leetcode.com/explore/*"]
		},
		{
			"css": ["store.css"],
			"matches": ["https://leetcode.com/store/*"]
		},
		{
			"css": ["subscribe.css"],
			"matches": ["https://leetcode.com/subscribe/*"]
		},
		{
			"css": ["list.css"],
			"matches": ["https://leetcode.com/list/"]
		},
		{
			"css": ["submission.css", "hljs-theme.css"],
			"matches": ["https://leetcode.com/submissions/*", "https://leetcode.com/progress/*"]
		},
		{
			"css": ["session.css"],
			"matches": ["https://leetcode.com/session/*"]
		},
		{
			"css": ["points.css"],
			"matches": ["https://leetcode.com/points/*"]
		},
		{
			"css": ["subscription.css"],
			"matches": ["https://leetcode.com/subscription/*"]
		},
		{
			"css": ["contribute.css"],
			"matches": ["https://leetcode.com/contribute/*"]
		},
		{
			"css": ["contest.css"],
			"matches": ["https://leetcode.com/contest/*"]
		},
		{
			"css": ["profile.css"],
			"matches": ["https://leetcode.com/profile/*"]
		}
	]
}
